# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API 配置
VITE_API_BASE_URL=http://localhost:5001

# AI 提供者配置

# 1. Ollama (本地部署 - 推薦免費使用)
# 請先安裝 Ollama: https://ollama.ai/
VITE_OLLAMA_BASE_URL=http://localhost:11434
VITE_OLLAMA_MODEL=llama3.1:8b
# 其他可用模型: qwen:7b, mistral:7b, gemma:7b, phi3:mini

# 1.1 Ollama 雲端部署 (可選 - 讓其他使用者也能使用)
# 如果您有雲端伺服器，可以部署 Ollama 服務
VITE_OLLAMA_CLOUD_URL=https://your-ollama-server.com

# 2. Hugging Face (免費開源模型)
# 請從 https://huggingface.co/settings/tokens 獲取您的 API Key
VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key_here
VITE_HUGGINGFACE_MODEL=meta-llama/Llama-3.1-8B-Instruct
# 其他可用模型: Qwen/Qwen2-7B-Instruct, mistralai/Mistral-7B-Instruct-v0.2

# 3. OpenAI API (有免費額度)
# 請從 https://platform.openai.com/api-keys 獲取您的 API Key
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-3.5-turbo

# 注意：Ollama 不需要 API Key，完全免費使用
